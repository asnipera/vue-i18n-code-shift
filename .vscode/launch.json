  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "findChineseText",
      "program": "${workspaceFolder}/src/utils/findChineseText.js",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "extract",
      "program": "${workspaceFolder}/src/commands/extract/index.js",
      "skipFiles": ["<node_internals>/**"]
